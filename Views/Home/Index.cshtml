@model IEnumerable<BadImageFormatException>
@{
    ViewData["Title"] = "Home Page";
}

<a asp-action="AddImage" class="btn btn-info">Add Profile</a>

@if (Model.Count() > 0)
{
    <table>
        <tr>
            <th>Picture</th>
            <th>User Name</th>
        </tr>
        @foreach (var item in Model)
        {
            <td>
                @Html.DisplayFor(m => item.Data)
            </td>
        }
    </table>
}